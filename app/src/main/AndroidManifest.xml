<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.umn.test3" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_plannerly"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_plannerly_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Test3" >
        <activity
            android:name=".MonthlyView"
            android:exported="true"
            android:label="Monthly Calendar"
            android:theme="@style/Theme.Test3.NoActionBar" />
        <activity
            android:name=".DailyCalendarActivity"
            android:exported="true"
            android:label="Daily Calendar" />
        <activity
            android:name=".WeekViewActivity"
            android:exported="true"
            android:label="Weekly Calendar" />
        <activity
            android:name=".AddActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>